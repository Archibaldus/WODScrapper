<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Daily Workout</title>
    <link href="https://fonts.googleapis.com/css2?family=Quicksand:wght@400;700&display=swap" rel="stylesheet"> 
    <link rel="stylesheet" type="text/css" href="style.css">
</head>
<body>

    <div class="main-content">
        <h1>Daily Workouts</h1>
        <% wodDates.forEach((element, index) => { %>
            <% let wod = myWods[index] %> 
            <% wod = wod.replace(/(\r\n|\n|\r)/gm, '<br>') %> 
            <div>
                <p class="wodDate"><%= element %></p>
                <div class="actualWod"><%- wod %> </div>

                <label for="notizen">Notizen</label>
                <input type="textare" id="notizen" name="notizen">

                <br>
                <hr>
            </div>  
        <% }); %>
    </div>
    
</body>
</html>

